<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<SplitPane dividerPositions="0.2" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.cnpuvache.gba.tile_manager.gui.ComponentsTabController">
   <items>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0">
         <children>
            <ListView fx:id="screensListView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
      <Pane fx:id="componentsCanvasPane" prefHeight="200.0" prefWidth="200.0">
         <children>
            <Canvas fx:id="componentsCanvas" height="200.0" width="200.0" />
         </children>
      </Pane>
   </items>
</SplitPane>
